<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Anomaly</li>
        </mods>

        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationConditional">
                    <xpath>Defs/FactionDef[defName="HoraxCult"]/xenotypeSet</xpath>
                    <nomatch Class="PatchOperationAdd">
                        <xpath>Defs/FactionDef[defName="HoraxCult"]</xpath>
                        <value>
                            <xenotypeSet>
                                <xenotypeChances></xenotypeChances>
                            </xenotypeSet>
                        </value>
                    </nomatch>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/FactionDef[defName="HoraxCult"]/xenotypeSet/xenotypeChances</xpath>
                    <value>
                        <BX_Bernael>0.08</BX_Bernael>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>